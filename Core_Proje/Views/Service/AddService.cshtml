
@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<br />
<div class="container mt-5">
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <form method="post">
                <div class="mb-3">
                    <input type="text" class="form-control" id="title" name="Title" placeholder="Hizmet Adı" />
